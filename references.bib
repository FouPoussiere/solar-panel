@thesis{ZhaoBoWenJiYuShenDuXueXiDeSiDingQueXianJianCeJiShuYanJiu2025,
  type = {硕士学位论文},
  title = {基于深度学习的丝锭缺陷检测技术研究},
  author = {{赵博文}},
