@article{LinEnhancedYOLOv8Model2025,
  title = {An {{Enhanced YOLOv8 Model}} with {{Symmetry-Aware Feature Extraction}} for {{High-Accuracy Solar Panel Defect Detection}}},
  author = {Lin, Xiaoxia and Xiao, Xinyue and Sun, Lin and Yang, Xiaodong and Leng, Chunwei and Li, Yan and Niu, Zhenyu and Meng, Yingzhou and Gong, Weihao},
  date = {2025-07},
  journaltitle = {Symmetry},
  volume = {17},
