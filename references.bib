@thesis{LiuTaoJiYuShenDuXueXiDeCiWaZaiXianJianCeJiShuYanJiu2022,
  type = {硕士学位论文},
  title = {基于深度学习的磁瓦在线检测技术研究},
  author = {{刘涛}},
  namea = {{叶炜}},
  nameatype = {collaborator},
